---
title : KnowYourStack ASP.NET Core Feed
permalink: "aspnetcore/feed/"
---

<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
<channel>
        <title>{{ site.title }}</title>
        <description>{{ site.title }} - {{ site.author.name }}</description>
        <link>{{ site.production_url }}{{ site.rss_path }}</link>
        <link>{{ site.production_url }}</link>
        <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
        <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
        <ttl>1800</ttl>

        <item>
                <title>{{ title | xml_escape  }}</title>
                <description><![CDATA[
                    <h1>ASP.NET Blog & Tweets</h1>
                    <h2>.NET Web Development and Tools Blog</h2>
				
				    {% for feedItem in site.data.AspNetCore.Data.Blog.FeedItems limit: 5 %}
                            <p>{{ feedItem.Title }}
                            <p>(Published on {{ feedItem.PublishedOn | date_to_long_string }})</p>
                            <a href="{{ feedItem.Href }}">Read more</a>
				    {% endfor %}

                    <h1>GitHub</h1>
                    <h2>aspnet/Announcements</h2>
				    {% for eventItem in site.data.AspNetCore.Data.Announcements.Events limit: 10 %}
                            <p>{{ eventItem.RelatedDescription | strip_html }}</p>
                            <p>on {{ eventItem.CreatedAt | date_to_long_string }} by {{ eventItem.Actor }}</p>
                            <a href="{{ eventItem.RelatedUrl }}">Read more</a>
				    {% endfor %}
               
                    <h2>aspnet/Home</h2>
				    {% for eventItem in site.data.AspNetCore.Data.Home.Events limit: 10 %}
                            <p>{{ eventItem.RelatedDescription | strip_html }}</p>
                            <p>on {{ eventItem.CreatedAt | date_to_long_string }} by {{ eventItem.Actor }}</p>
                            <a href="{{ eventItem.RelatedUrl }}">Read more</a>
				    {% endfor %}

                    <h2>aspnet/Mvc</h2>
				    {% for eventItem in site.data.AspNetCore.Data.Mvc.Events limit: 10 %}
                            <p>{{ eventItem.RelatedDescription | strip_html }}</p>
                            <p>on {{ eventItem.CreatedAt | date_to_long_string }} by {{ eventItem.Actor }}</p>
                            <a href="{{ eventItem.RelatedUrl }}">Read more</a>
				    {% endfor %}

                    <h2>aspnet/Razor</h2>
				    {% for eventItem in site.data.AspNetCore.Data.Razor.Events limit: 10 %}
                            <p>{{ eventItem.RelatedDescription | strip_html }}</p>
                            <p>on {{ eventItem.CreatedAt | date_to_long_string }} by {{ eventItem.Actor }}</p>
                            <a href="{{ eventItem.RelatedUrl }}">Read more</a>
				    {% endfor %}

                    <h2>aspnet/SignalR</h2>
				    {% for eventItem in site.data.AspNetCore.Data.SignalR.Events limit: 10 %}
                            <p>{{ eventItem.RelatedDescription | strip_html }}</p>
                            <p>on {{ eventItem.CreatedAt | date_to_long_string }} by {{ eventItem.Actor }}</p>
                            <a href="{{ eventItem.RelatedUrl }}">Read more</a>
				    {% endfor %}

                    <h2>aspnet/Identity</h2>
				    {% for eventItem in site.data.AspNetCore.Data.Identity.Events limit: 10 %}
                            <p>{{ eventItem.RelatedDescription | strip_html }}</p>
                            <p>on {{ eventItem.CreatedAt | date_to_long_string }} by {{ eventItem.Actor }}</p>
                            <a href="{{ eventItem.RelatedUrl }}">Read more</a>
				    {% endfor %}

                    <h2>aspnet/Kestrel</h2>
				    {% for eventItem in site.data.AspNetCore.Data.Kestrel.Events limit: 10 %}
                            <p>{{ eventItem.RelatedDescription | strip_html }}</p>
                            <p>on {{ eventItem.CreatedAt | date_to_long_string }} by {{ eventItem.Actor }}</p>
                            <a href="{{ eventItem.RelatedUrl }}">Read more</a>
				    {% endfor %}

                ]]></description>
                <link>https://www.KnowYourStack.com/aspnetcore/</link>
                <guid>https://www.KnowYourStack.com/aspnetcore/</guid>
                <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
        </item>

</channel>
</rss>